<%- include('partials/head')%>
    <title>Login | Aphonistar</title>
</head>
<body> 
  <%- include('partials/nav')%>
    <section class="register-login">
        <div class="register-div">
            <p class="register-p">Log In</p>
            <form action="/login" class="register-form" method="POST">
                <div class="register-wrap">
                    <label for="phasionName" class="register-label">PhasionName</label>
                    <input class="register-input" type="text" name="username" id="phasionName" placeholder="Your PhasionName" autocomplete="off">
                </div>
                <div class="register-wrap">
                    <label for="password" class="register-label">Password</label>
                    <input class="register-input" type="password" name="password" id="password" placeholder="Enter Your Password" autocomplete="off">
                </div>
                <!-- <div class="form-group">
                    <label for="email">Email</label>
                    <input type="email" name="email" id="email" placeholder="Your Mail" autocomplete="off">
                </div> -->
                <div class="register-wrap">
                    <button class="register-button" type="submit" name="loginbtn">Sign In</button>
                </div>
            </form>
        </div>
    </section>
  <%- include('partials/footer')%>
</body>

</html>

    





